<div id="searchContainer">
    <form action="/" method="GET">
        <div class="inputRow">
            <label for="purl">Url</label>
            <input type="text" id="purl" name="playlist" placeholder="Playlist url" value="<%= locals.purl ? purl : '' %>">
        </div>
        <div class="inputRow">
            <label for="page">Page</label>
            <input type="number" id="page" name="page" step="1" value="<%= locals.currentPage ? currentPage : 1 %>">
        </div>
        <button type="submit"> Compare </button>
    </form>
</div>